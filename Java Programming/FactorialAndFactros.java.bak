class FactorialAndFactros
{
	public static void main(String[] args) 
	{
		int num=4;
		int fact=1;
		for (int i=1;i<=4;i++)
		{
		fact=fact*i;
		}
		System.out.println(fact);
			
		}
		for (int j=1;j<=fact;j++)
		{
			if (fact%j==0)
			{
				System.out.println(j);
			}
		}
	}
}
