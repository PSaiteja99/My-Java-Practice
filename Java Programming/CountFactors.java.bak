import java.util.Scanner;
class CountFactors
{
	public static void main(String[] args) 
	{
		Scanner sc =  new Scanner(System.in);
		System.out.print("Enter the number :");
		int n = sc.nextInt();
		int b=1;
		for (int i =1;i<=n;i++)
		{
			if(n%i=1)
			{
				b=b*i;
			}
		}
		System.out.println(b);
	}
}
