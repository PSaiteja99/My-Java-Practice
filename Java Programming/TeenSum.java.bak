import java.util.Scanner;
class TeenSum 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter 'a' value:");
		int a = sc.nextInt();
		System.out.print("Enter 'b' number:");
		int b = sc.nextInt();
		if(a>=13 && b<=19)
		{
			System.out.print(19);
		}
		else
		{
			System.out.print(+(a+b));
		}
		
	}
}
