import java.util.Scanner;
class ProductDigitsWhile
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Hello World!");
		int num = sc.nextInt();
		int pro=1;
		while(num!=0)
		{
			int ld=num%10;
			pro=pro*ld;
			num=num/10;
		}
		System.out.println(pro);
	}
}
