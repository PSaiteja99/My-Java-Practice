import java.util.Scanner;
class FactorsOf100
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number:");
		int x = sc.nextInt();
		for (int n=1;n<=x;n++)
		{
		System.out.println("factors of"+n);
		for (int i=1;i<=n;i++)
			{
			if(n%i==0)
				{
				System.out.println(i);
				}

			}
		}
	}
}